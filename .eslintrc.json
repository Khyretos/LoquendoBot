{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "indent":["error", "tab"],
        "no-tabs":"off",
        "prefer-destructuring": ["error", {
            "AssignmentExpression": {
                "array": false,
                "object": true
            }
        }],
				"no-console": ["error", { "allow": ["warn", "error"] }]
    }
}
